<?xml version="1.0" encoding="UTF-8"?>						<!-- This XML is running  dockerGoogleSearchChrome and dockerGoogleSearchFirefox on docker container-->
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes">
  <test thread-count="5" name="Test" parallel="classes">
    <classes>
      <class name="com.docker.dockerGoogleSearchChrome"/>	<!-- Will run in parallel and will go directly to the chrome node -->
      <class name="com.docker.dockerGoogleSearchFireFox"/>	<!-- Will run in parallel and will go directly to the firefox node -->
    </classes>
  </test> 													<!-- Test -->
</suite> 													<!-- Suite -->
